{
  "cSpell.words": [
    "contentlayer",
    "darkmode",
    "heroicons",
    "Katex",
    "nord",
    "nprogress",
    "rehype",
    "tailwindcss"
  ],
  "vim.statusBarColorControl": false,
  "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
  "vim.statusBarColors.insert": ["#BF616A", "#ffffff"],
  "vim.statusBarColors.visual": ["#B48EAD", "#a00f5f"],
  "vim.statusBarColors.visualline": ["#B48EAD", "#a00f5f"],
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": ["#343f4f", "#afafaf"],
  "vim.statusBarColors.searchinprogressmode": ["#343f4f", "#afafaf"],
  "vim.statusBarColors.easymotionmode": ["#343f4f", "#afafaf"],
  "vim.statusBarColors.easymotioninputmode": ["#343f4f", "#afafaf"],
  "vim.statusBarColors.surroundinputmode": ["#343f4f", "#afafaf"],
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "1",
  "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/bin/fcitx5-remote",
  "vim.autoSwitchInputMethod.switchIMCmd": "/usr/bin/fcitx5-remote -t {im}",
  "pasteImage.basePath": "${projectRoot}/public/image",
  "pasteImage.path": "${projectRoot}/public/image",
  "pasteImage.prefix": "image/",
  "pangu.auto_space_on_save": true,
  "pangu.auto_space_on_save_ext": ["mdx"]
}
